{
  "name": "less-cluster",
  "version": "0.4.4",
  "description": "Parallel, directory-based execution of lessc.",
  "main": "index.js",
  "bin": {
    "less-cluster": "./bin/less-cluster"
  },
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "undef": true,
    "node": true
  },
  "scripts": {
    "pretest": "jshint bin lib test",
    "test": "istanbul test --print both ./node_modules/mocha/bin/_mocha",
    "posttest": "istanbul check-coverage --statements 60 --functions 50 --branches 50 --lines 60"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zillow/less-cluster.git"
  },
  "keywords": [
    "less",
    "css",
    "lesscss",
    "compiler",
    "cluster"
  ],
  "author": "Daniel Stockman <daniels@zillow.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "~1.8.0",
    "istanbul": "~0.1.44",
    "jshint": "~2.1.11",
    "mocha": "~1.13.0",
    "sinon": "~1.7.3",
    "sinon-chai": "~2.4.0"
  },
  "dependencies": {
    "nopt": "~2.1.1",
    "glob": "~3.2.1",
    "less": "git://github.com/evocateur/less.js.git#normalize-urls",
    "mkdirp": "~0.3.5"
  }
}
