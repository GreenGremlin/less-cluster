{
  "name": "less-cluster",
  "version": "0.4.7",
  "description": "Parallel, directory-based execution of lessc.",
  "main": "index.js",
  "bin": {
    "less-cluster": "./bin/less-cluster"
  },
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "undef": true,
    "node": true
  },
  "scripts": {
    "pretest": "jshint bin lib test",
    "test": "istanbul cover --print both ./node_modules/mocha/bin/_mocha -- test test/integration",
    "posttest": "istanbul check-coverage --statements 90 --functions 90 --branches 90 --lines 90"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zillow/less-cluster.git"
  },
  "keywords": [
    "less",
    "css",
    "lesscss",
    "compiler",
    "cluster"
  ],
  "author": "Daniel Stockman <daniels@zillow.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^1.9.1",
    "istanbul": "^0.2.9",
    "jshint": "^2.5.0",
    "mocha": "^1.18.2",
    "rimraf": "^2.2.8",
    "sinon": "^1.9.1",
    "sinon-chai": "^2.5.0"
  },
  "dependencies": {
    "nopt": "~2.1.2",
    "glob": "~3.2.7",
    "graceful-fs": "~2.0.1",
    "less": "git://github.com/evocateur/less.js.git#normalize-urls",
    "mkdirp": "~0.3.5"
  }
}
